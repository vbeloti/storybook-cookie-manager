{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "noEmitOnError": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "target": "esnext",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true
  }
}
